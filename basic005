///Array of structure

#include<stdio.h>
#include<conio.h>
struct person{
    int age;
    float salary;
};
int main()
{
    struct person person[5];
    int i;

    for(i=0; i<5; i++)
    {
        printf("Enter Information for Person %d\n", i+1);
        printf("Enter age: ");
        scanf("%d", &person[i].age);
        printf("Enter Salary: ");
        scanf("%f", &person[i].salary);
    }
    for(i=0; i<5; i++)
    {
        printf("\nInformation Person %d\n", i+1);
        printf("Age = %d\n",person[i].age);
        printf("Salary = %f",person[i].salary);
    }


    getch();
}
