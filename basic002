#include<stdio.h>
///Global structure
struct person{
    int age;
    float salary;
};
int main()
{
    struct person person1, person2;
    printf("Enter Person 1 age: ");
    scanf("%d", &person1.age);
    printf("Enter Person 1 Salary: ");
    scanf("%f", &person1.salary);
    printf("\nPerson 1: \nAge = %d\nSalary =%.2f\n",person1.age,person1.salary);

    printf("\nEnter Person 2 age: ");
    scanf("%d", &person2.age);
    printf("Enter Person 2 Salary: ");
    scanf("%f", &person2.salary);
    printf("\nPerson 2: \nAge = %d\nSalary =%.2f\n",person2.age,person2.salary);

    return 0;
}
